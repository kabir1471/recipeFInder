{
  "name": "recipeFinder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:unit": "npm run test --",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:build:device": "detox build --configuration android.device.debug",
    "e2e:test:device": "detox test --configuration android.device.debug",
    "e2e:android": "npm run e2e:build:android && npm run e2e:test:android",
    "e2e:device": "npm run e2e:build:device && npm run e2e:test:device"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-vector-icons/ionicons": "^12.3.0",
    "@react-native-vector-icons/material-design-icons": "^12.4.0",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@tanstack/query-async-storage-persister": "^5.90.9",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-persist-client": "^5.90.9",
    "axios": "^1.13.2",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-offline": "^6.0.2",
